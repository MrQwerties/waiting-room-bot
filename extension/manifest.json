{
  "name": "Waiting Room Bot for Zoom Meetings",
  "version": "0.1.0",
  "description": "Waiting Room Bot for Zoom Meetings",
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "background": {
